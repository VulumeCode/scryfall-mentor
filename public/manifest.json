{
    "manifest_version": 2,
    "name": "Scryfall Mentor",
    "version": "0.1.3",
    "browser_specific_settings": {
        "gecko": {
            "id": "scryfall-mentor@VulumeCode",
            "update_url": "https://raw.githubusercontent.com/VulumeCode/scryfall-mentor/master/package.json"
        }
    },
    "description": "Scryfall query builder.",
    "homepage_url": "https://github.com/VulumeCode/scryfall-mentor",
    "icons": {
        "32": "scryfall.svg"
    },
    "sidebar_action": {
        "default_icon": "scryfall.svg",
        "default_title": "Scryfall Mentor",
        "default_panel": "index.html"
    },
    "permissions": [
        "history",
        "tabs",
        "storage",
        "webNavigation"
    ],
    "commands": {
        "_execute_sidebar_action": {
            "suggested_key": {
                "default": "Alt+Ctrl+S"
            }
        }
    }
}